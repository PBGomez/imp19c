	#Widget to show industrial goods and manage industries
	type industrial_goods_widget = margin_widget {
		size = { 138 70 }
		margin = { 4 4 }
		widget = {
			size = { 100% 100% }
			using = list_item
			blockoverride "Margin" { margin = { 2 2 } }
		}
		widget = {
			size = { 100% 33 }
			background = {
				shaderfile = "gfx/FX/pdxgui_default.shader"
				using = red_color
				using = portrait_x32y32bd_alpha
				alpha = 0.25
				margin = { -2 -2 }
			}
			textbox = {
				# Amount of income or expenses from exporting/importing this tradegood
				autoresize = yes
				parentanchor = right
				position = { -4 8 }
				using = BaseFontM
				using = Black_FontColor
				fontsize = 16
				block "CashBalanceText" {
					text = "£x.xxx"
				}
				block "CashBalanceTooltip" {
					tooltip = "Cashbalancetooltip"
				}
			}
		}

		textbox = {
			# Number of factories in this governorship
			position = { 95 45 }
			fontsize = 22
			using = BaseFontM
			using = Black_FontColor
			block "VisibleTrigger" {
				visible = no # Has variable factories_assigned_INDUSTRY
			}
			block "AssignedText" {
				text = "X"
			}
		}
		
		list_button = {
			size = { 60 60 }
			oversound = "event:/SFX/UI/Generic/sfx_ui_generic_mouse_over"
			clicksound = "event:/SFX/UI/Character/Unique/sfx_ui_character_loan_request"
			parentanchor = vcenter
			block "On_click" { }
			block "On_rightclick" { }
			block "ButtonTooltip" { tooltip = "Button tooltip" }
			margin_widget = {
				margin = { 2 2 }
				size = { 100% 100% }
				alpha = 2.0
				flowcontainer = {
					position = { 5 35}
					direction = horizontal
					textbox = {
						block "StockpileText" { text = "XXX" }
						autoresize = yes
						parentanchor = bottom|left
						position = { 5 0 }
						fontsize = 16
						using = BaseFontM
						using = Black_FontColor
						block "BalanceTooltip"
						{
							tooltip = "Balance"
						}
					}
					textbox = {
						block "BalanceText" { text = "+-X" }
						autoresize = yes
						parentanchor = bottom|left
						position = { 0 0 }
						using = BaseFontM
						fontsize = 16
						using = Black_FontColor
						block "BalanceTooltip"
						{
							tooltip = "Balance"
						}
					}
				}
				icon = {
					size = { 36 36 }
					block "Icon" 
					{ 
						texture = "gfx/interface/icons/_default.dds"
						parentanchor = vcenter|top 
						position = { 6 -3 }
					}
					parentanchor = top|hcenter
				}
			}
		}
	}
