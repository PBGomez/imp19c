	#IMP19C TYPES 
	##WAR SCORE BAR
	type fixed_size_progressbar_war_score = widget {
		size = { 480 90 }
		widget = {
			size = { 100% 20 }
			parentanchor = bottom
			icon = {
				using = flat_background
				size = { 4 20 }
			}
			margin_widget = {
				size = { 100% 20 }
				margin = { 6 0 }
				tooltip = "[WarOverview.GetWarScoreTooltip]"
				icon = {
					name = "progress_frame"
					texture = "gfx/interface/base_parts/progressbar_background_frame.dds"
					
					minimumsize = { 0 7 }
					size = { 100% 100% }
					color = { 0.7 0.7 0.7 1 }
					block "ProgressAlpha" { alpha = 0.5 }
				}
				progressbar = {
					name = "progress_value"
					size = { 100% 100% }
					block "ProgressColor" { using = green_color }
					block "Value" { value = 1 }
					marker = {
						container = {
							name = "progress_marker2"           
							alwaystransparent = yes
							icon = {
								texture = "gfx/interface/icons/shared_icons/plus.dds"
								size = { 23 23 }
								position = { -24 0 }
								color = { 0 0 0 1 }
								block "PlusMarker" { visible = no }
							}
							icon = {
								texture = "gfx/interface/icons/shared_icons/plus.dds"
								size = { 23 23 }
								position = { -30 0 }
								color = { 0 0 0 1 }
								block "PlusMarker" { visible = no }
							}
							icon = {
								texture = "gfx/interface/icons/shared_icons/minus.dds"
								size = { 23 23 }
								position = { -24 0 }
								color = { 0.4 0 0 1 }
								block "MinusMarker" { visible = no }
							}
							icon = {
								texture = "gfx/interface/icons/shared_icons/minus.dds"
								size = { 23 23 }
								position = { -30 0 }
								color = { 0.4 0 0 1 }
								block "MinusMarker" { visible = no }
							}
						}
					}
				}
			}
			icon = {
				using = flat_background
				parentanchor = right
				size = { 4 20 }
			}
		}
		flowcontainer = {
			direction = vertical
			#textbox = {
			#	size = { 220 80 }
			#	using = BaseFontM
			#	using = Black_FontColor
			#	datacontext = "[WarOverview]"
			#	text = "WAR_DATE_DETAILS"
			#	multiline = yes
			#}
			textbox = {
				size = { 220 60 }
				align = bottom
				multiline = yes
				using = BaseFontM
				using = Black_FontColor
				datacontext = "[WarOverview]"
				text = "WAR_WARGOAL_DETAILS"
				tooltip = "WAR_OVERVIEW_WAR_GOAL_TOOLTIP"
			}
		}
		flowcontainer = {
			visible = "[WarOverview.GetWar.HasTickingWarScore]"
			parentanchor = right
			position = { 0 10 }
			direction = vertical
			textbox = {
				size = { 200 26 }
				align = right
				using = BaseFontM
				using = Black_FontColor
				datacontext = "[WarOverview]"
				text = "WARSCORE_DETAILS"
				tooltip = "[WarOverview.GetTickingWarScoreTooltip]"
			}
			flowcontainer = {
				textbox = {
					name = "Text"
					size = { 160 36 }
					fontsize = 24
					align = vcenter|right
					elide = right
					using = BaseFontM
					using = Black_FontColor
					block "Text" { text = "888.88%" }
					visible = "[Not(WarOverview.GetWar.IsCivilWar)]"
					tooltip = "[WarOverview.GetWarScoreTooltip]"
				}
				icon = {
					texture = "gfx/interface/arrows_strip.dds"
					position = { 8 18 }
					framesize = { 50 50 }
					size = { 40 40 }
					frame = "[WarOverview.GetTickingWarScoreChangeFrame]"
					tooltip = "[WarOverview.GetTickingWarScoreChangeTooltip]"
					visible = "[WarOverview.HasTickingWarScore]"
				}
			}
		}
		icon = {
			parentanchor = hcenter
			framesize = { 50 50 }
			size = { 60 60 }
			
			block "Icon" { texture = "gfx/interface/icons/shared_icons/leader.dds" }
			icon = {
				texture = "gfx/interface/icons/shared_icons/plus.dds"
				block "UpIndicator" { visible = no }
				position = { -2 12 }
			}
			icon = {
				texture = "gfx/interface/icons/shared_icons/minus.dds"
				block "DownIndicator" { visible = no }
				position = { -2 12 }
			}
		}
	}
	#Bigger size plus button
	type button_plus_war = widget {
		size = { 35 35 }
		
		icon = {
			name = "Shadow"
			position = { -1 1 }
			texture = "gfx/interface/base_parts/roung_shadow_52.dds"
			shaderfile = "gfx/FX/pdxgui_default.shader"
